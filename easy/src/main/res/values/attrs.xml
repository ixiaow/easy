<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleView">
        <attr name="title_text" format="string|reference" />
        <attr name="title_text_color" format="reference|color|string" />
        <attr name="title_text_size" format="dimension|reference" />
        <attr name="left_icon" format="reference" />
        <attr name="title_background" format="reference|string" />
        <attr name="title_menu" format="reference" />
    </declare-styleable>
    <declare-styleable name="ExpandView">
        <attr name="title_text" />
        <attr name="title_size" format="dimension" />
        <attr name="title_text_color" />
        <attr name="indicator_expand" format="reference" />
        <attr name="indicator_collapse" format="reference" />
        <attr name="expand_layout" format="reference" />
        <attr name="item_background" format="reference|color|integer" />
    </declare-styleable>

    <declare-styleable name="NoticeAngle">
        <attr name="notice_text_size" format="dimension|reference" />
        <attr name="notice_text_color" format="color|reference" />
    </declare-styleable>
</resources>